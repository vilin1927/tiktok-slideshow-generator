# RapidAPI Key for TikTok scraper
RAPIDAPI_KEY=your_rapidapi_key_here

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Rate Limiting (optional - tune for your API tier)
# MAX_CONCURRENT: Parallel requests (default: 3, increase for higher tiers)
# RPM_LIMIT: Requests per minute (default: 30, increase for Tier 2+)
GEMINI_MAX_CONCURRENT=3
GEMINI_RPM_LIMIT=30

# Google OAuth credentials (for Drive uploads)
GOOGLE_OAUTH_CLIENT_ID=your_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_client_secret_here

# Google Drive folder ID (optional - uploads to root if not set)
PARENT_FOLDER_ID=your_folder_id_here

# Logging Configuration
LOG_DIR=./logs
LOG_LEVEL=DEBUG

# Admin Panel Password
ADMIN_PASSWORD=your_secure_password_here
